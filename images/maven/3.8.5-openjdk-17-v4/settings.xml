<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <pluginGroups>
  </pluginGroups>
  <proxies>
  </proxies>
  <servers>
    <server>
      <id>1_ke-yc-snapshots</id>
      <password>${env.MAVEN_RW_PASSWORD}</password>
      <username>${env.MAVEN_RW_LOGIN}</username>
    </server>
    <server>
      <id>2_ke-yc-old-snapshots</id>
      <password>${env.MAVEN_RO_PASSWORD}</password>
      <username>${env.MAVEN_RO_LOGIN}</username>
    </server>
    <server>
      <id>1_ke-yc-releases</id>
      <password>${env.MAVEN_RW_PASSWORD}</password>
      <username>${env.MAVEN_RW_LOGIN}</username>
    </server>
    <server>
      <id>2_ke-yc-old-releases</id>
      <password>${env.MAVEN_RO_PASSWORD}</password>
      <username>${env.MAVEN_RO_LOGIN}</username>
    </server>
  </servers>
  <mirrors>
  </mirrors>
  <profiles>
    <profile>
      <id>KE-YC</id>
      <repositories>
        <repository>
          <id>1_ke-yc-releases</id>
          <name>Repository for KE RELEASES</name>
          <url>https://nexus.infra.cluster.kznexpess.com/repository/maven-releases/</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </repository>
        <repository>
          <id>1_ke-yc-snapshots</id>
          <name>Repository for KE SNAPSHOTs</name>
          <url>https://nexus.infra.cluster.kznexpess.com/repository/maven-snapshots/</url>
          <releases>
            <enabled>false</enabled>
            <updatePolicy>always</updatePolicy>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
        </repository>
        <repository>
          <id>2_ke-yc-old-releases</id>
          <name>Repository for KE OLD RELEASES</name>
          <url>https://nexus.infra.cluster.kznexpess.com/repository/old-maven-releases/</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </repository>
        <repository>
          <id>2_ke-yc-old-snapshots</id>
          <name>Repository for KE OLD SNAPSHOTS</name>
          <url>https://nexus.infra.cluster.kznexpess.com/repository/old-maven-snapshots/</url>
          <releases>
            <enabled>false</enabled>
            <updatePolicy>always</updatePolicy>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
        </repository>
      </repositories>
      <properties>
        <altSnapshotDeploymentRepository>1_ke-yc-snapshots::default::https://nexus.infra.cluster.kznexpess.com/repository/maven-snapshots/</altSnapshotDeploymentRepository>
        <altReleaseDeploymentRepository>1_ke-yc-releases::default::https://nexus.infra.cluster.kznexpess.com/repository/maven-releases/</altReleaseDeploymentRepository>
      </properties>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>KE-YC</activeProfile>
  </activeProfiles>
</settings>